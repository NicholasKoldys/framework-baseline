{
  "name": "javascript-development-environment",
  "version": "1.1.0",
  "author": "Nicholas Koldys",
  "description": "JavaScript development environment",
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "prestart": "node buildScripts/startMessage.js",
    "start": "pnpm run start-mockapi open:src lint:watch test:watch",
    "open:src": "node buildScripts/srcServer.js",
    "mock:userdata": "node buildScripts/generateMockData",
    "prestart-mockapi": "pnpm run mock:userdata",
    "start-mockapi": "pnpm exec json-server --watch src/api/db.json --port 3301",
    "lint": "pnpm exec esw webpack.config.* src buildScripts --color",
    "lint:watch": "pnpm run lint -- --watch",
    "test": "pnpm exec mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "pnpm run test -- --watch",
    "clean:dist": "rimraf ./dist && mkdir dist",
    "prebuild": "pnpm clean:dist test lint",
    "build": "node buildScripts/build.js",
    "postbuild": "node buildScripts/distServer.js",
    "preprodtest": "pnpm run clean:dist",
    "prodtest": "pnpm run open:src",
    "predevbuild": "pnpm run clear:dist",
    "devbuild": "pnpm exec webpack build --config ./webpack.config.dev.mjs --stats verbose"
  },
  "scriptsComments": {
    "scripts-comment": ""
  },
  "keywords": [],
  "devDependencies": {
    "@babel/cli": "7.21.5",
    "@babel/core": "7.22.1",
    "@babel/node": "^7.22.1",
    "@babel/preset-env": "7.22.4",
    "@babel/register": "7.21.0",
    "@types/webpack-dev-middleware": "^5.3.0",
    "babel-loader": "9.1.2",
    "chai": "4.3.7",
    "chalk": "4.1.2",
    "compression": "1.7.4",
    "copy-webpack-plugin": "^5.1.2",
    "css-loader": "6.8.1",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-watch": "8.0.0",
    "express": "4.18.2",
    "faker": "^6.6.6",
    "html-webpack-plugin": "5.5.1",
    "jsdom": "^22.1.0",
    "json-schema-faker": "0.5.3",
    "json-server": "0.17.3",
    "localtunnel": "2.0.2",
    "mini-svg-data-uri": "^1.4.4",
    "mocha": "10.2.0",
    "rimraf": "5.0.1",
    "style-loader": "3.3.3",
    "surge": "0.23.1",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "6.1.1",
    "workbox-webpack-plugin": "^7.0.0",
    "workbox-window": "^7.0.0"
  },
  "dependenciesComments": {
    "devDependencies-omments": ""
  }
}